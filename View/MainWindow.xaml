<Window x:Class="SnapNET.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:local="clr-namespace:SnapNET"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="resButton" Content="Button" HorizontalAlignment="Left" Margin="220,150,0,0" VerticalAlignment="Top" Width="120" Height="75" Click="ResButton_Click"/>
        <Button x:Name="winButton" Content="Button" HorizontalAlignment="Left" Margin="445,150,0,0" VerticalAlignment="Top" Width="120" Height="75" Click="WinButton_Click"/>
    </Grid>
</Window>
